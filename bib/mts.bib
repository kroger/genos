@Article{	  kurth92:mosaic,
  jstor_articletype ={primary_article},
  title =        {Mosaic Polyphony: Formal Balance, Imbalance, and
                  Phrase Formation in the Prelude of Schoenberg's
                  Suite, Op. 25},
  author =       {Kurth, Richard B.},
  journal =      {Music Theory Spectrum},
  volume =       14,
  number =       2,
  jstor_formatteddate ={Autumn, 1992},
  pages =        {188--208},
  url =          {http://www.jstor.org/stable/746107},
  issn =         01956167,
  abstract =     {Order-number mosaics and their associated
                  pitch-class mosaics are applied to twelve-tone rows
                  in Schoenberg's music, and the notion of mosaic
                  polyphony is developed to study the rhythmic
                  structures thus highlighted in a musical
                  excerpt. The mosaic polyphony is then used as a
                  basis for observations about the music's formal
                  construction. Several analyses, all drawn from the
                  Prelude from the Suite for Piano, Op. 25, show how
                  rhythmic, metrical, and registral features,
                  sometimes involving powerful symmetries, affect
                  phrase formation and cohesion, and give the music
                  its special dynamic balance and characteristic
                  fluidity.},
  year =         1992,
  publisher =    {University of California Press on behalf of the,
  file =         {746107.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1992 University of California Press}
}

@Article{	  cohn92:the,
  jstor_articletype ={primary_article},
  title =        {The Autonomy of Motives in Schenkerian Accounts of
                  Tonal Music},
  author =       {Cohn, Richard},
  journal =      {Music Theory Spectrum},
  volume =       14,
  number =       2,
  jstor_formatteddate ={Autumn, 1992},
  pages =        {150--170},
  url =          {http://www.jstor.org/stable/746105},
  issn =         01956167,
  abstract =     {This paper explores the tension between Schenkerian
                  theory, which mandates that motivic relations be
                  fully subsumed by the Ursatz hierarchy, and
                  Schenkerian analytic practice, which treats motives
                  as partly autonomous entities. Part 1 reviews the
                  status of motives in the writings of Schenker and
                  his successors. Part 2 explores three ways that
                  practice departs from prescription. Part 3 explores
                  three possible consequences of this situation, and
                  concludes by recommending a revision of some major
                  tenets of Schenkerian theory.},
  year =         1992,
  publisher =    {University of California Press on behalf of the,
  file =         {746105.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1992 University of California Press}
}

@Article{	  boss92:schoenbergs,
  jstor_articletype ={primary_article},
  title =        {Schoenberg's Op. 22 Radio Talk and Developing
                  Variation in Atonal Music},
  author =       {Boss, Jack},
  journal =      {Music Theory Spectrum},
  volume =       14,
  number =       2,
  jstor_formatteddate ={Autumn, 1992},
  pages =        {125--149},
  url =          {http://www.jstor.org/stable/746104},
  issn =         01956167,
  abstract =     {A common process contributes to organizing the
                  interval structures in both tonal music and the
                  atonal music of Arnold Schoenberg, a process he
                  called "developing variation." This article shows
                  how developing variation generates an atonal piece,
                  highlighting ways in which atonal and tonal
                  variation are similar. The article systematically
                  extends remarks that Schoenberg made in a radio
                  lecture on the Four Orchestral Songs, Op. 22. An
                  analysis of the first stanza of "Seraphita" Op. 22
                  No. 1 shows that two considerations motivate the
                  chains of overlap-generated successions that
                  gradually increase and decrease in remoteness from
                  the motivic source: Schoenberg's obligations to
                  delimit sections within the form and to characterize
                  those sections in accordance with the text.},
  year =         1992,
  publisher =    {University of California Press on behalf of the,
  file =         {746104.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1992 University of California Press}
}

