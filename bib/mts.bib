@Article{	  snyder90:entropy,
  jstor_articletype ={primary_article},
  title =        {Entropy as a Measure of Musical Style: The Influence
                  of a Priori Assumptions},
  author =       {Snyder, John L.},
  journal =      {Music Theory Spectrum},
  volume =       12,
  number =       1,
  jstor_formatteddate ={Spring, 1990},
  pages =        {121--160},
  url =          {http://www.jstor.org/stable/746148},
  issn =         01956167,
  abstract =     {The present article examines some of the premises
                  underlying previous applications of information
                  theory to musical analysis, and suggests that those
                  studies have failed because of faulty preanalytical
                  assumptions. Chief among these are confusion of the
                  concepts of pitch class and scale degree, dependence
                  on notational devices (key signatures), and
                  confusion over the treatment of modal
                  differences. Tonal music is most appropriately
                  analyzed in terms of scale degrees, referring all
                  pitch materials in a work or movement to the
                  principal key for meaning. These steps yield results
                  both more discriminating and more plausible than
                  those obtained in previous studies.},
  year =         1990,
  publisher =    {University of California Press on behalf of the,
  file =         {746148.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1990 University of California Press}
}

@Article{	  harrison90:rhetoric,
  jstor_articletype ={primary_article},
  title =        {Rhetoric and Fugue: An Analytical Application},
  author =       {Harrison, Daniel},
  journal =      {Music Theory Spectrum},
  volume =       12,
  number =       1,
  jstor_formatteddate ={Spring, 1990},
  pages =        {1--42},
  url =          {http://www.jstor.org/stable/746145},
  issn =         01956167,
  abstract =     {The article begins by critiquing traditional
                  understandings of musical rhetoric, offering in
                  their place a conception of rhetoric rooted in the
                  classical model of rhetoric as persuasive
                  discourse. Fugue in general is studied in light of
                  its affinity to this model. The fugue to Bach's
                  Toccata, S. 915, is then closely analyzed as
                  rhetorical discourse. The notion of rhetorical
                  status, or issue, is brought to bear upon the
                  subject of the fugue, which exposes the
                  compositional problems that the fugue addresses with
                  specific "arguments." The article concludes with
                  additional observations on the nature of musical
                  rhetoric.},
  year =         1990,
  publisher =    {University of California Press on behalf of the,
  file =         {746145.pdf}
