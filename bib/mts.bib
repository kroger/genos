}

@Article{	  bass07:enharmonic,
  jstor_articletype ={primary_article},
  title =        {Enharmonic Position Finding and the Resolution of
                  Seventh Chords in Chromatic Music},
  author =       {Bass, Richard},
  journal =      {Music Theory Spectrum},
  volume =       29,
  number =       1,
  jstor_formatteddate ={Spring, 2007},
  pages =        {73--100},
  url =          {http://www.jstor.org/stable/4499872},
  issn =         01956167,
  abstract =     {Tonal harmonic theory as inherited from
                  nineteenth-century sources is limited in its ability
                  to explain irregular resolutions of seventh chords
                  because it requires that all harmonies be understood
                  in relation to a particular key.  Approaching these
                  resolutions instead based on voice-leading
                  transformations in which characteristic dissonances
                  in seventh chords (i.e., the tritone, augmented
                  sixth, and diminished seventh) progress to consonant
                  intervals contained in a major or minor triad makes
                  it possible to categorize and compare both regular
                  and irregular seventh-chord resolutions. The
                  transformations entail locating all possible
                  positions for these successions of interval classes
                  in progressions from seventh chords to triads, often
                  involving enharmonic reinterpretation. The RES
                  function introduced in this study offers a
                  systematic means of accounting for the plausible
                  resolutions of seventh chords and is useful in the
                  analysis of chromatic music, as illustrated in
                  examples from the works of nineteenth-century
                  composers, including complete structures by CÃ©sar
                  Franck and Richard Strauss.},
  year =         2007,
  publisher =    {University of California Press on behalf of the,
  file =         {4499872.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2007 University of California Press}
}

@Article{	  hook07:cross-type,
  jstor_articletype ={primary_article},
  title =        {Cross-Type Transformations and the Path Consistency
                  Condition},
  author =       {Hook, Julian},
  journal =      {Music Theory Spectrum},
  volume =       29,
  number =       1,
  jstor_formatteddate ={Spring, 2007},
  pages =        {1--39},
  url =          {http://www.jstor.org/stable/4499870},
  issn =         01956167,
  abstract =     {Two kinds of extensions to David Lewin's
                  transformation theory are proposed. First,
                  cross-type transformations transform one sort of
                  object to another: for instance, mappings from
                  triads to seventh chords. Special cases include GIS
                  homomorphisms and interscalar transpositions and
                  inversions. The second extension relaxes Lewin's
                  requirement that the products of transformations
                  along two paths from one node to another in a
                  transformation graph must match. It is argued that
                  this path consistency condition is unnecessarily
                  restrictive; several alternatives are
                  considered. Examples from Bach to Bartok illustrate
                  musical relationships that can be most effectively
                  modeled through the use of cross-type
                  transformations and non-path-consistent graphs.},
  year =         2007,
  publisher =    {University of California Press on behalf of the,
  file =         {4499870.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2007 University of California Press}
}

