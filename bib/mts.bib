@Article{	  burkholder90:the,
  jstor_articletype ={primary_article},
  title =        {The Critique of Tonality in the Early Experimental
                  Music of Charles Ives},
  author =       {Burkholder, J. Peter},
  journal =      {Music Theory Spectrum},
  volume =       12,
  number =       2,
  jstor_formatteddate ={Autumn, 1990},
  pages =        {203--223},
  url =          {http://www.jstor.org/stable/746168},
  issn =         01956167,
  abstract =     {In dozens of small experimental works, Ives tried
                  out new musical techniques that he often later used
                  in larger compositions. But these experiments also
                  have a critical side: by achieving familiar ends
                  through novel means, Ives seeks to demonstrate the
                  artificiality of accepted conventions. In his early
                  experiments, Ives critiques tonality and its basic
                  concepts, including tonic-dominant polarity,
                  prolongation, tertial chords, and cadences. These
                  pieces use polytonality, whole-tone fields,
                  symmetrical chords, and other post-tonal techniques,
                  but in every case Ives establishes a pitch center
                  and deploys his material in ways that strongly
                  parallel the structure of simple tonal music.},
  year =         1990,
  publisher =    {University of California Press on behalf of the,
  file =         {746168.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1990 University of California Press}
}

@Article{	  block90:pitch-class,
  jstor_articletype ={primary_article},
  title =        {Pitch-Class Transformation in Free Jazz},
  author =       {Block, Steven},
  journal =      {Music Theory Spectrum},
  volume =       12,
  number =       2,
  jstor_formatteddate ={Autumn, 1990},
  pages =        {181--202},
  url =          {http://www.jstor.org/stable/746167},
  issn =         01956167,
  abstract =     {Set-theoretic methodology is applied to the music of
                  Ornette Coleman, John Coltrane, Cecil Taylor, and
                  Anthony Braxton in order to reveal the wide variety
                  of pitch-class transformation present in free
                  jazz. Each composer's music has been classified
                  somewhat differently by other analysts--Coltrane's
                  as modal, Coleman's as diatonic, and Taylor's as
                  nontonal--yet all the improvisations examined here
                  are shown to be based on tightly constructed
                  conceptions which make use of such twentieth-century
                  constructs as the multiplicative operation,
                  transformation of embedded chords, and the use of a
                  small number of transformational operations which
                  control the course of the composition.},
  year =         1990,
  publisher =    {University of California Press on behalf of the,
  file =         {746167.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1990 University of California Press}
}

