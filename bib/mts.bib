@Article{	  smith94:brahms,
  jstor_articletype ={primary_article},
  title =        {Brahms and Schenker: A Mutual Response to Sonata
                  Form},
  author =       {Smith, Peter H.},
  journal =      {Music Theory Spectrum},
  volume =       16,
  number =       1,
  jstor_formatteddate ={Spring, 1994},
  pages =        {77--103},
  url =          {http://www.jstor.org/stable/745831},
  issn =         01956167,
  abstract =     {In approaching sonata form, Brahms and Schenker both
                  struck a compromise between a quintessential
                  late-nineteenth-century view of musical form in
                  which continuous linear evolution is valued above
                  elements of sectionalization, and their sensitivity
                  to the realities of a formal type based in part on
                  the dramatic delineation of a parallel thematic
                  design. This essay explores the difficulties
                  Schenker encountered in the derivation of sonata
                  form from a single Ursatz and reveals that he was
                  anachronistically forcing the ideal of organic unity
                  onto the large-scale formal conventions of the
                  classical period.  However, these very difficulties
                  evaporate when his method of analysis is applied to
                  a number of Brahms's more unusual sonata
                  forms. Through an overlap of the retransition and
                  the beginning of the recapitulation and a
                  reinterpretation of the sonata principle, Brahms
                  achieved creatively the specific type of organic
                  unity Schenker had difficulty demonstrating in his
                  analyses of late-eighteenth-century music.},
  year =         1994,
  publisher =    {University of California Press on behalf of the,
  file =         {745831.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1994 University of California Press}
}

@Article{	  lester94:an,
