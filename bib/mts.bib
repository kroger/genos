@Article{	  galand95:form,
  jstor_articletype ={primary_article},
  title =        {Form, Genre, and Style in the Eighteenth-Century
                  Rondo},
  author =       {Galand, Joel},
  journal =      {Music Theory Spectrum},
  volume =       17,
  number =       1,
  jstor_formatteddate ={Spring, 1995},
  pages =        {27--52},
  url =          {http://www.jstor.org/stable/745763},
  issn =         01956167,
  abstract =     {In order to exemplify a rapprochement between
                  analytical theory and historical inquiry,
                  Schenkerian concepts of form are applied to a
                  style-historical investigation of the
                  eighteenth-century rondo. Schenker himself, despite
                  his innovations, too often follows traditional
                  nineteenth-century Formenlehre in narrowly equating
                  an individual genre with a particular formal
                  technique, even if historically the one did not
                  necessarily imply the other. The interrelationship
                  of form and genre is too complex to be ascribed to a
                  shared middle-ground paradigm.  Nevertheless,
                  Schenkerian theory allows a clearer picture to
                  emerge of the ways in which composers fused rondo
                  and ritornello characteristics with binary variants
                  of sonata form. Formal procedures previously
                  considered sui generis are actually fairly common
                  eighteenth-century treatments of the finale,
                  characteristic of composers like Vanhal and Steffan
                  as well as Mozart.},
  year =         1995,
  publisher =    {University of California Press on behalf of the,
  file =         {745763.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1995 University of California Press}
}

@Article{	  schubert95:a,
  jstor_articletype ={primary_article},
  title =        {A Lesson from Lassus: Form in the Duos of 1577},
  author =       {Schubert, Peter N.},
  journal =      {Music Theory Spectrum},
  volume =       17,
  number =       1,
  jstor_formatteddate ={Spring, 1995},
  pages =        {1--26},
  url =          {http://www.jstor.org/stable/745762},
  issn =         01956167,
  abstract =     {This article demonstrates how contrapuntal
                  techniques contribute to the formal structure of
                  Lassus's twenty-four famous duos. Three types of
                  contrapuntal events are identified in each piece:
                  time interval of imitation, fuga (type of melodic
                  variation), and "blocks" (contrapuntal combinations,
                  usually varied by transposition or invertible
                  counterpoint). Occurrences of these events are
                  mapped onto time lines for all twenty-four
                  pieces. The time lines suggest conclusions regarding
                  Lassus's normative formal procedures. These
                  procedures can in some cases be related to text and,
                  in others, taken as suggestions for expressive
                  performance.},
  year =         1995,
  publisher =    {University of California Press on behalf of the,
  file =         {745762.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1995 University of California Press}
}

