  url =          {http://www.jstor.org/stable/745828},
  issn =         01956167,
  abstract =     {Scholars have disputed the extent to which
                  Schopenhauerian metaphysics influenced the creation
                  of Wagner's Ring. The controversy revolves around
                  the so-called "Schopenhauerian" ending, verses that
                  Wagner did not set to music because, in his opinion,
                  "their meaning is already expressed with the
                  greatest precision in the effect of the
                  drama-sounding-in-music." The first part of this
                  essay reviews Wagner's changing attitude towards his
                  Nibelung project and considers the ending of
                  GÃ¶tterdÃ¤mmerung in the light of the
                  "Schopenhauerian" verses. The second part examines
                  several of the important musical themes and
                  addresses the adequacy of the traditional
                  "redemption by love" interpretation. The final
                  section offers a musical analysis of the Immolation
                  Scene which supports the "Schopenhauerian" reading.},
  year =         1994,
  publisher =    {University of California Press on behalf of the,
  file =         {745828.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1994 University of California Press}
}

