  author =       {Karl, Gregory},
  journal =      {Music Theory Spectrum},
  volume =       19,
  number =       1,
  jstor_formatteddate ={Spring, 1997},
  pages =        {13--34},
  url =          {http://www.jstor.org/stable/745997},
  issn =         01956167,
  abstract =     {An analytical method based on structuralist
                  principles is developed for the exploration of an
                  abstract and specifically musical form of dramatic
                  organization, called musical plot, underlying works
                  of the nineteenth and twentieth centuries. The
                  fundamental semantic units of musical plots are
                  binary relationships akin to literary foils and
                  functional sequences, standard patterns of
                  interaction among musical units, usually themes and
                  motives, cast in abstract dramatic roles. The first
                  movement of Beethoven's Piano Sonata, op. 57, is
                  analyzed by this method as a drama of mental life in
                  which an antagonistic force within the first theme
                  undermines the interests of the work's persona.},
  year =         1997,
  publisher =    {University of California Press on behalf of the,
  file =         {745997.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1997 University of California Press}
}

@Article{	  carter97:stravinskys,
  jstor_articletype ={primary_article},
  title =        {Stravinsky's "Special Sense": The Rhetorical Use of
                  Tonality in "The Rake's Progress"},
  author =       {Carter, Chandler},
  journal =      {Music Theory Spectrum},
  volume =       19,
  number =       1,
  jstor_formatteddate ={Spring, 1997},
  pages =        {55--80},
  url =          {http://www.jstor.org/stable/745999},
  issn =         01956167,
  abstract =     {To understand more fully what Donald Mitchell calls
                  Stravinsky's "special sense of the past," this
                  article analyzes four relatively short passages from
                  The Rake's Progress, paying particular attention to
                  foreground chord succession and voice
                  leading. Conventional voice-leading analysis, pc-set
                  theory, motivic analysis, and recent post-tonal
                  approaches to voice leading are used-and ultimately
                  incorporated-to assess the role(s) that tonal
                  conventions play within the broader scope of
                  Stravinsky's post-tonal harmonic language. Viewed
                  broadly, this pluralistic approach offers multiple
                  perspectives from which to measure seemingly
                  irreconcilable differences and to identify cross
                  references-thereby revealing features that mediate
                  between divergent styles. More importantly, this
                  approach acknowledges the stylistic conflict,
                  tension, and interplay so typical of the music of
                  Stravinsky (and so many other twentieth-century
                  composers) without regarding these features as
                  ancillary or exceptional.},
  year =         1997,
  publisher =    {University of California Press on behalf of the,
  file =         {745999.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1997 University of California Press}
}

