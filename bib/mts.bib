@Article{	  cook01:theorizing,
  jstor_articletype ={primary_article},
  title =        {Theorizing Musical Meaning},
  author =       {Cook, Nicholas},
  journal =      {Music Theory Spectrum},
  volume =       23,
  number =       2,
  jstor_formatteddate ={Autumn, 2001},
  pages =        {170--195},
  url =          {http://www.jstor.org/stable/745985},
  issn =         01956167,
  abstract =     {This article offers a model of musical meaning that
                  allows for the cultural construction of musical
                  meaning, while at the same time acknowledging the
                  existence of constraints upon the meanings any given
                  music may support under any given circumstances; in
                  this way it aims to fill the void between existing
                  approaches that understand musical meaning as either
                  inherent or socially constructed. I illustrate the
                  argument through Tovey's and McClay's contrasting
                  readings of the recapitulation in the first movement
                  of Beethoven's Ninth Symphony, and suggest some ways
                  in which theory and analysis may enter into a
                  constructive relationship with the broadened
                  critical agenda of contemporary musicology.},
  year =         2001,
  publisher =    {University of California Press on behalf of the,
  file =         {745985.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2001 University of California Press}
}

@Article{	  horlacher01:running,
  jstor_articletype ={primary_article},
  title =        {Running in Place: Sketches and Superimposition in
                  Stravinsky's Music},
  author =       {Horlacher, Gretchen},
  journal =      {Music Theory Spectrum},
  volume =       23,
  number =       2,
  jstor_formatteddate ={Autumn, 2001},
  pages =        {196--216},
  url =          {http://www.jstor.org/stable/745986},
  issn =         01956167,
  abstract =     {This article considers Stravinsky's use of
                  superimposed, repeating motivic fragments from the
                  perspective of his sketches and drafts. It describes
                  a common procedure where sketches with single
                  statements of fragments are expanded by the
                  insertion of repetitions from within. Thus, the
                  repetitions originate as interpolations rather than
                  additions, becoming members of a larger formal
                  framework.  The paper describes how the fragments
                  may interact contrapuntally to create a series of
                  ordered episodes that counteract their reiterative
                  origins. It considers sketches from Symphony of
                  Psalms and Symphony in Three Movements.},
  year =         2001,
  publisher =    {University of California Press on behalf of the,
  file =         {745986.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2001 University of California Press}
}

@Article{	  leydon01:debussys,
  jstor_articletype ={primary_article},
  title =        {Debussy's Late Style and the Devices of the Early
                  Silent Cinema},
  author =       {Leydon, Rebecca},
  journal =      {Music Theory Spectrum},
  volume =       23,
  number =       2,
  jstor_formatteddate ={Autumn, 2001},
  pages =        {217--241},
  url =          {http://www.jstor.org/stable/745987},
  issn =         01956167,
  abstract =     {The article situates Debussy's late works within the
                  context of the technologies of the early silent
                  cinema.  Cinematic techniques developed by French
                  film-makers during Debussy's lifetime can provide
                  the basis for a model of continuity and succession
                  in this music and suggest some new ways of
                  approaching form in his late style. Cinema was of
                  considerable consequence in France during Debussy's
                  mature career, and the technical devices of the
                  cinema constituted a markedly new way of
                  representing time and space. Very few of the films
                  from the late 1890s and early 1900s have survived,
                  but the extant film reviews and contemporary
                  criticism provide a vivid sense of the early cinema,
                  its reception, and its characteristic devices.
                  Particular cinematic editing techniques are noted as
                  models for Debussy's own characteristic repertoire
                  of musical devices. Finally, some of the connections
                  are made between cinema and Debussy's nationalism in
                  the years surrounding the first world war.},
  year =         2001,
  publisher =    {University of California Press on behalf of the,
  file =         {745987.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2001 University of California Press}
}

