  jstor_formatteddate ={Autumn, 2006},
  pages =        {211--231},
  url =          {http://www.jstor.org/stable/4499860},
  issn =         01956167,
  abstract =     {A study of syncopation in American popular music was
                  carried out based on analyses of sound recordings
                  spanning the period 1890 to 1939. Sample measures
                  were randomly selected and the presence and rhythmic
                  character of syncopations were tabulated. Several
                  trend-related hypotheses were tested. While some
                  changes in the patterns of syncopation were evident
                  over the 50-year period of the study, the principal
                  change was an increase in the quantity of
                  syncopation rather than an increase in the variety
                  of syncopated patterns.},
