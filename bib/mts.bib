@Article{	  capuzzo04:neo-riemannian,
  jstor_articletype ={primary_article},
  title =        {Neo-Riemannian Theory and the Analysis of Pop-Rock
                  Music},
  author =       {Capuzzo, Guy},
  journal =      {Music Theory Spectrum},
  volume =       26,
  number =       2,
  jstor_formatteddate ={Autumn, 2004},
  pages =        {177--199},
  url =          {http://www.jstor.org/stable/4488738},
  issn =         01956167,
  abstract =     {This article outlines the use of neo-Riemannian
                  operations (NROs) for the analysis of certain
                  pop-rock chord progressions whose features invite a
                  transformational approach. After presenting the NROs
                  used in the paper, I delineate the general features
                  of the progressions under discussion, distinguish
                  the progressions from the late-Romantic progressions
                  analyzed with NROs by Richard Cohn, Brian Hyer,
                  Henry Klumpenhouwer, and David Lewin, and contrast
                  pc parsimony (one or two pcs shared by two triads)
                  with p parsimony (one or two pitches shared by two
                  triads).  I then offer a series of analyses, which
                  fall into three categories: sequences, progressions
                  with chromatic lines from 8Ì‚ or 5Ì‚, and a song
                  that combines triads and seventh chords. I close
                  with an analysis of a complete song.},
  year =         2004,
  publisher =    {University of California Press on behalf of the,
  file =         {4488738.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2004 University of California Press}
}

@Article{	  everett04:a,
  jstor_articletype ={primary_article},
  title =        {A Royal Scam: The Abstruse and Ironic Bop-Rock
                  Harmony of Steely Dan},
  author =       {Everett, Walter},
  journal =      {Music Theory Spectrum},
  volume =       26,
  number =       2,
  jstor_formatteddate ={Autumn, 2004},
  pages =        {201--235},
  url =          {http://www.jstor.org/stable/4488739},
  issn =         01956167,
  abstract =     {Steely Dan is nominally a rock band, but their
                  melodic idiosyncracies, rhythmic surfaces, and
                  harmonic and voice-leading techniques are direct
                  descendants of early modern jazz, making theirs
                  arguably the most tonally complex of any rock music
                  with broad popularity. This article illustrates how
                  the group's sophisticated and enigmatic chord
                  constructions, along with a mix of feigned, oblique,
                  and incongruous turns of harmony, intensify the
                  linear aspect of voice-leading connections in the
                  experimental manner of the great bop musicians, and
                  convey the band's penchant for the recondite and the
                  ironic.},
