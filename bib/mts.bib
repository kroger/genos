@Article{	  schultz08:melodic,
  jstor_articletype ={primary_article},
  title =        {Melodic Contour and Nonretrogradable Structure in
                  the Birdsong of Olivier Messiaen},
  author =       {Schultz, Rob},
  journal =      {Music Theory Spectrum},
  volume =       30,
