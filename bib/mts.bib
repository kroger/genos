}

@Article{	  lambert90:interval,
  jstor_articletype ={primary_article},
  title =        {Interval Cycles as Compositional Resources in the
                  Music of Charles Ives},
  author =       {Lambert, J. Philip},
  journal =      {Music Theory Spectrum},
  volume =       12,
  number =       1,
  jstor_formatteddate ={Spring, 1990},
  pages =        {43--82},
  url =          {http://www.jstor.org/stable/746146},
  issn =         01956167,
  abstract =     {In the "experimental" music of Charles Ives,
                  interval patterns and particular intervallic
                  combinations often serve as primary structural
                  forces. Particularly prevalent are cyclic
                  intervallic repetitions, reflecting compositional
                  concerns for coherence through repetition and
                  pitch-class variety. Ives experiments both with the
                  familiar interval cycles and with cycles of two
                  alternating intervals, or "combination cycles."
                  Musical usages include straightforward cyclic
                  presentations as well as developments of cyclically
                  generated structural frameworks.  In his most
                  sophisticated cyclic experiments, including In re
                  con moto et al, On the Antipodes, and the Universe
                  Symphony, cycles and cyclic principles are central
                  to the musical and metaphorical substance, mirroring
                  pervasive elements of Ives's attitudes toward art
                  and nature.},
  year =         1990,
  publisher =    {University of California Press on behalf of the,
  file =         {746146.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1990 University of California Press}
}

