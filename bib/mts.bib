@Article{	  temperley08:hypermetrical,
  jstor_articletype ={primary_article},
  title =        {Hypermetrical Transitions},
  author =       {Temperley, David},
  journal =      {Music Theory Spectrum},
  volume =       30,
  number =       2,
  jstor_formatteddate ={Fall, 2008},
  pages =        {305--325},
  url =          {http://www.jstor.org/stable/20532955},
  issn =         01956167,
  abstract =     {Most hypermetrical shifts in common-practice music
                  are shifts of "duple phase"-- between "odd-strong"
                  hypermeter (in which odd-numbered measures are
                  strong) and "even-strong," or vice versa. I
                  distinguish between sudden and gradual shifts,
                  focusing on the latter type: A gradual hypermetrical
                  shift is a situation in which the musical cues for
                  hypermeter shift gradually from one structure to
                  another. (This does not necessarily mean our
                  perception of the shift is gradual, a separate and
                  complex issue which I also address.) Drawing an
                  analogy with transitions between keys, I call such
                  shifts "hypermetrical transitions." I examine
                  hypermetrical transitions in pieces by Mozart,
                  Beethoven, Chopin, and Mendelssohn, considering the
                  way the shift is carried out and its function within
                  the passage and the piece as a whole.},
  year =         2008,
  publisher =    {University of California Press on behalf of the,
  file =         {20532955.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2008 University of California Press}
}

@Article{	  slottow08:carl,
  jstor_articletype ={primary_article},
  title =        {Carl Ruggles and Charles Seeger: Strict vs. Free
                  Imitation in Ruggle's Canons},
  author =       {Slottow, Stephen P.},
  journal =      {Music Theory Spectrum},
  volume =       30,
  number =       2,
  jstor_formatteddate ={Fall, 2008},
  pages =        {283--303},
  url =          {http://www.jstor.org/stable/20532954},
  issn =         01956167,
  abstract =     {Carl Ruggles first used canon in Lilacs, the second
                  movement of "Men and Mountains" (1927). Thereafter
                  canons grew increasingly important in his work,
                  growing in size and complexity until, in "Organum"
                  (1947), they comprise most of the
                  composition. Ruggles's use of canon appears to run
                  counter to his preference for freely mutating shapes
                  and his dislike of systematic devices. However,
                  Ruggles's canons always strike a balance between
                  strict and free repetition, in part reflecting the
                  influence of Charles Seeger's dissonant
                  counterpoint. This article discusses canons in
                  "Lilacs", "Sun-Treader", and "Evocation" IV.},
  year =         2008,
  publisher =    {University of California Press on behalf of the,
  file =         {20532954.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2008 University of California Press}
}

@Article{	  galand08:some,
  jstor_articletype ={primary_article},
  title =        {Some Eighteenth-Century Ritornello Scripts and Their
                  Nineteenth-Century Revivals},
  author =       {Galand, Joel},
  journal =      {Music Theory Spectrum},
  volume =       30,
  number =       2,
  jstor_formatteddate ={Fall, 2008},
  pages =        {239--282},
  url =          {http://www.jstor.org/stable/20532953},
  issn =         01956167,
  abstract =     {Theorists at least as far back as Tovey have noted a
                  number of movements by Schubert and Brahms that seem
                  anomalous, with respect both to the rest of their
                  works and to the way we generally understand the
                  formal conventions of the classical Viennese
                  instrumental music that constituted their horizon of
                  expectations. I submit that these pieces are
                  considered sui generis only because they have not
                  been analyzed in light of more flexible 18th-century
                  techniques for combining ritornello and binary
                  procedures, whether in rondos, in concerto forms, or
                  in concertante chamber music. This paper uses
                  Schenkerian analysis together with concepts drawn
                  from historical theory to reveal structural
                  similarities between ritornello forms by Haydn and
                  Mozart on the one hand, and Schubert and Brahms on
                  the other. The paper draws comparisons between
                  specific pieces (for instance, the finale of
                  Mozart's Sonata in C Minor, K. 457, and the first
                  movement of Brahms's Trio in C Minor, op. 101) not
                  for the purpose of claiming an empirically
                  verifiable instance of influence but rather to
                  demonstrate strikingly similar fusions of ritornello
                  and sonata principles. On a more general level, this
                  paper is intended as a contribution to Schenkerian
                  studies, specifically towards the application of the
                  theory not only to the analysis of individual pieces
                  but also to broader considerations of rhythm, style,
                  form, and genre.},
  year =         2008,
  publisher =    {University of California Press on behalf of the,
  file =         {20532953.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2008 University of California Press}
}

@Article{	  bain08:messy,
  jstor_articletype ={primary_article},
  title =        {"Messy Structure"? Multiple Tonal Centers in the
                  Music of Machaut},
  author =       {Bain, Jennifer},
  journal =      {Music Theory Spectrum},
  volume =       30,
  number =       2,
  jstor_formatteddate ={Fall, 2008},
  pages =        {195--237},
  url =          {http://www.jstor.org/stable/20532952},
  issn =         01956167,
  abstract =     {This paper develops a theory of large-scale tonal
                  structure for the secular, polyphonic music of
                  Machaut.  Allowing for the possibility of multiple
                  tonal centers in individual works, it compares
                  structural sonorities across Machaut's output and
                  accounts for a matrix of relationships rather than
                  single categories for each piece. Proposing that
                  coherence and aesthetic value can arise through
                  layers of relationships, it provides a tool for
                  discerning larger patterns of structure and for
                  determining standard versus anomalous behavior in
                  individual songs. Closer readings of the texts of
                  three songs and the relationship between text and
                  large-scale structure demonstrate the application of
                  the theory.},
  year =         2008,
  publisher =    {University of California Press on behalf of the,
  file =         {20532952.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2008 University of California Press}
}

