@Article{	  boone00:marking,
  jstor_articletype ={primary_article},
  title =        {Marking Mensural Time},
  author =       {Boone, Graeme M.},
  journal =      {Music Theory Spectrum},
  volume =       22,
  number =       1,
  jstor_formatteddate ={Spring, 2000},
  pages =        {1--43},
  url =          {http://www.jstor.org/stable/745851},
  issn =         01956167,
  abstract =     {A controversy, sometimes explicit and sometimes
                  implicit, has long haunted the study of mensural
                  music. Is mensural rhythm, or is it not, subject to
                  regular rhythmic hierarchies? If it is, then what is
                  their nature? "Marking Mensural Time" aims to
                  resolve central aspects of this controversy by
                  clarifying certain of its conceptual underpinnings
                  and by introducing a fresh approach to the
                  discussion of mensural rhythm, grounded in the
                  concept of punctuality. On the basis of mensural
                  theory up to 1600, together with punctually oriented
                  analysis of five musical examples spanning this era,
                  it is argued that regular rhythmic hierarchies do
                  exist in mensural music, and that they are closely
                  related to, as well as significantly distinct from,
                  metrical hierarchies found in common-practice
                  music. In the course of discussion, the concept of
                  tactus, beginning with its first major articulation
                  by Adam of Fulda (1490), is also given fresh
                  examination, as a construct illustrating both the
                  connections and the distinctions between mensural
                  and common-practice conceptualizations.},
  year =         2000,
  publisher =    {University of California Press on behalf of the,
  file =         {745851.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2000 University of California Press}
}

