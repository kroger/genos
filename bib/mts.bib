@Article{	  lubben93:schenker,
  jstor_articletype ={primary_article},
  title =        {Schenker the Progressive: Analytic Practice in "Der
                  Tonwille"},
  author =       {Lubben, Joseph},
  journal =      {Music Theory Spectrum},
  volume =       15,
  number =       1,
  jstor_formatteddate ={Spring, 1993},
  pages =        {59--75},
  url =          {http://www.jstor.org/stable/745909},
  issn =         01956167,
  abstract =     {The analytic essays and graphs published by Heinrich
                  Schenker in Der Tonwille (1921-24) are unique among
                  his output. They integrate sophisticated
                  descriptions of harmonic-contrapuntal structure with
                  accounts of the simultaneous influence of a
                  multitude of musical parameters. Examination of key
                  aspects of three of these analyses reveals the
                  degree of musical insight that Schenker was able to
                  express within this flexible methodology, and
                  provides further evidence of the need to modify the
                  role of the Ursatz in current Schenkerian analytic
                  practice.},
  year =         1993,
  publisher =    {University of California Press on behalf of the,
  file =         {745909.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1993 University of California Press}
}

@Article{	  clough.douthett.ea93:early,
  jstor_articletype ={primary_article},
  title =        {Early Indian Heptatonic Scales and Recent Diatonic
                  Theory},
  author =       {Clough, John and Douthett, Jack and Ramanathan,
                  N. and Rowell, Lewis},
  journal =      {Music Theory Spectrum},
  volume =       15,
  number =       1,
  jstor_formatteddate ={Spring, 1993},
  pages =        {36--58},
  url =          {http://www.jstor.org/stable/745908},
  issn =         01956167,
  abstract =     {Two questions are addressed: (1) By what path did
                  ancient Indian musicians make their way from a
                  "chromatic" universe of 22 microtonal divisions of
                  the octave (the Å›rutis) to a "diatonic" set of
                  seven degrees? and (2) What features do the
                  resulting scale structures have in common with later
                  versions of the diatonic scale in the West? The
                  authors examine the selection principles that appear
                  to have guided these musicians, and explore some of
                  the remarkable features and implications of their
                  solutions. The two basic early Indian heptatonic
                  scales (sa-grÄma and ma-grÄma) share a
                  number of features with the Western diatonic scale:
                  (1) distinct step sizes which are consecutive
                  integers, (2) dual tetrachords, (3) exactly one
                  tritone, (4) distinct sizes of fifths which are
                  consecutive integers, (5) a maximal number of
                  consonant fifths (consistent with feature 4), and
                  (6) first- or second-order maximal evenness (as
                  defined by Clough and Douthett). There are three
                  smaller feature sets, each including two or three of
                  the above features, that serve to define both the
                  early Indian heptatonics and the Western diatonic:
                  given the appropriate chromatic cardinality (22 or
                  12), each feature set corresponds either (a) to all
                  and only the heptatonics specified in the early
                  Indian treatises plus two permissible altered
                  versions or (b) uniquely to the Western diatonic.},
  year =         1993,
  publisher =    {University of California Press on behalf of the,
  file =         {745908.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 1993 University of California Press}
}

@Article{	  smyth93:balanced,
  jstor_articletype ={primary_article},
  title =        {"Balanced Interruption" and the Formal Repeat},
  author =       {Smyth, David},
  journal =      {Music Theory Spectrum},
  volume =       15,
  number =       1,
  jstor_formatteddate ={Spring, 1993},
  pages =        {76--88},
  url =          {http://www.jstor.org/stable/745910},
  issn =         01956167,
  abstract =     {This essay explores certain interactions between
