@Article{	  cramer02:schoenbergs,
  jstor_articletype ={primary_article},
  title =        {Schoenberg's "Klangfarbenmelodie": A Principle of
                  Early Atonal Harmony},
  author =       {Cramer, Alfred},
  journal =      {Music Theory Spectrum},
  volume =       24,
  number =       1,
  jstor_formatteddate ={Spring, 2002},
  pages =        {1--34},
  url =          {http://www.jstor.org/stable/1556086},
  issn =         01956167,
  abstract =     {Contrary to conventional opinion, Schoenberg's
                  writings of 1911 and 1951 imply that he envisioned
                  "Klangfarbenmelodie" as a type of harmonic
                  progression. The term arose as Schoenberg imagined a
                  theory of atonal music along the lines of the
                  "Klang" model of tonality. Where the "Klang" tied a
                  chord's tones to the pitch of the fundamental,
                  "Klangfarbenmelodie" involved the possibility of
                  tones being sensed without reference to pitch. In
                  this light, two interpretations of Schoenberg's
                  early atonal music are proposed. The first treats a
                  tone's "Klangfarbe" as a sensation encompassing
                  pitch, making the music resemble phonetic
                  sounds. The second involves emergent timbres of
                  chords. Schoenberg's atonal harmonic designs often
                  promote the fusion of chords and the
                  imperceptibility of pitch within them.},
  year =         2002,
  publisher =    {University of California Press on behalf of the,
  file =         {1556086.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2002 University of California Press}
}

