@Article{	  stoecker02:klumpenhouwer,
  jstor_articletype ={primary_article},
  title =        {Klumpenhouwer Networks, Trichords, and Axial
                  Isography},
  author =       {Stoecker, Philip},
  journal =      {Music Theory Spectrum},
  volume =       24,
  number =       2,
  jstor_formatteddate ={Autumn, 2002},
  pages =        {231--245},
  url =          {http://www.jstor.org/stable/1556101},
  issn =         01956167,
  abstract =     {Recent writings on transformational theory by David
                  Lewin and Henry Klumpenhouwer have introduced the
                  "Klumpenhouwer network," which recognizes both
                  transpositional and inversional relationships within
                  a single pitch-class collection. In constructing
                  their networks, Lewin and Klumpenhouwer have
                  proposed that an interval within each network
                  remains invariant while inversional relationships
                  change. There are, however, some relationships that
                  cannot be accounted for by conventional network
                  isography. Graphs may be musically and
                  orthographically connected and yet not fit one of
                  Lewin's or Klumpenhouwer's definitions. This article
                  demonstrates another kind of isography, "axial
                  isography," wherein an inversional relationship is
                  preserved among trichordal Klumpenhouwer networks.},
  year =         2002,
  publisher =    {University of California Press on behalf of the,
  file =         {1556101.pdf}
                  Society for Music Theory},
  copyright =    {Copyright Â© 2002 University of California Press}
}

