@Article{	  neal07:narrative,
  jstor_articletype ={primary_article},
  title =        {Narrative Paradigms, Musical Signifiers, and Form as
                  Function in Country Music},
  author =       {Neal, Jocelyn R.},
  journal =      {Music Theory Spectrum},
  volume =       29,
  number =       1,
  jstor_formatteddate ={Spring, 2007},
  pages =        {41--72},
  url =          {http://www.jstor.org/stable/4499871},
  issn =         01956167,
  abstract =     {Within country music's songwriting practices,
                  musical form and harmonic structure often become
                  functional contributors to a song's story; these
                  combinations of form and theme can be modeled as
                  distinct narrative paradigms. This essay defines a
                  Time-Shift narrative paradigm that relates a central
                  trope in country music's texts to formal,
                  structural, and poetic devices. It then presents
                  analyses of several songs to show how the use of the
                  narrative paradigm reinforces the core identity of
                  country as a genre, and connects art, biography, and
                  interpretation.},
  year =         2007,
  publisher =    {University of California Press on behalf of the,
  file =         {4499871.pdf}
                  Society for Music Theory},
