@PhDThesis{	  bordini03:teoria,
  author =       {Ricardo Mazzini Bordini},
  title =        {A teoria pós-tonal e o processador de classes de
                  notas aplicados à composição musical: um tutorial},
  school =       {Universidade Federal da Bahia. Escola de Música},
  year =         2003,
  type =         {Tese (doutorado)},
  address =      {Salvador},
  abstract =     {Este trabalho tem por objeto a elaboração de um
                  Tutorial utilizando recursos de hipertexto para
                  vincular: 1) conceitos e definições da Teoria
                  Pós-tonal, 2) as operações do Processador de Classes
